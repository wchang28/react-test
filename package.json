{
  "name": "react-test",
  "version": "1.0.0",
  "description": "React Tester",
  "scripts": {
    "transpile": "tsc",
    "browserify": "browserify -o ./dist/ui/bundle.js ./dist/ui/main.js",
    "uglify": "uglifyjs ./dist/ui/bundle.js -o ./public/js/bundle.min.js --compress --mangle",
    "package-ui": "npm run browserify && npm run uglify",
    "postinstall": "npm run transpile && npm run package-ui",
    "browserify-dev": "browserify -o ./public/js/bundle.min.js ./dist/ui/main.js",
    "build-dev": "npm run transpile && npm run browserify-dev",
    "start": "node ./dist/server/app.js"
  },
  "keywords": [
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wchang28/react-test.git"
  },
  "author": "Wen Chang",
  "license": "MIT",
  "dependencies": {
    "@progress/kendo-data-query": "^1.5.4",
    "@types/codemirror": "0.0.91",
    "@types/express": "^4.17.6",
    "@types/jquery": "^3.3.38",
    "@types/node": "^14.0.2",
    "@types/react": "^16.9.35",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-datepicker": "^2.11.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-json-tree": "^0.6.11",
    "@types/react-modal": "^3.10.6",
    "@types/react-paginate": "^6.2.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/shortid": "0.0.29",
    "clsx": "^1.1.0",
    "codemirror": "^5.51.0",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "interval-polling": "git+https://github.com/wchang28/interval-polling.git",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-codemirror2": "^6.0.0",
    "react-data-grid": "^6.1.0",
    "react-data-grid-addons": "^6.1.0",
    "react-data-grid-addons-extension": "git+https://github.com/wchang28/react-data-grid-addons-extension.git",
    "react-datepicker": "^2.12.1",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "react-json-tree": "^0.11.2",
    "react-modal": "^3.11.2",
    "react-paginate": "^6.3.2",
    "react-router-dom": "^5.2.0",
    "shortid": "^2.2.15",
    "uglify-css": "git+https://github.com/wchang28/uglify-css.git",
    "xlsx": "^0.16.2"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "typescript": "^3.9.5",
    "uglify-js": "^3.7.6"
  }
}
