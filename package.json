{
  "name": "react-test",
  "version": "1.0.0",
  "description": "React Tester",
  "main": "index.js",
  "scripts": {
    "transpile": "tsc",
    "browserify": "browserify ./dist/ui/main.js -o ./dist/ui/bundle.js",
    "uglify": "uglifyjs ./dist/ui/bundle.js -o ./public/js/bundle.min.js --compress --mangle",
    "package-ui": "npm run browserify && npm run uglify",
    "postinstall": "npm run transpile && npm run package-ui",
    "start": "node ./dist/server/app.js"
  },
  "keywords": [
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wchang28/react-test.git"
  },
  "author": "Wen Chang",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.5.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "uglify-js": "^3.7.6"
  }
}
